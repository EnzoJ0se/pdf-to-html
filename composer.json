{
    "name": "enzojose/pdf-to-html",
    "description": "A simple PHP library to convert PDF files to HTML using Poppler Utils.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Enzo Jose",
            "email": "enzojose.reus@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Enzojose\\PdfToHtml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "symfony/process": "^7.3"
    },
    "require-dev": {
        "pestphp/pest": "4.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    }
}
